;
; Test for core features
; values
;

(if (equal? (let-values ([() (values)]) 1) 1)
    (void)
    (exit 1))

(if (equal? (let-values ([(x) (values 1)]) x) 1)
    (void)
    (exit 2))

(if (equal? (let-values ([(x y) (values 1 2)]) (list x y)) '(1 2))
    (void)
    (exit 3))

(if (equal? (let-values ([(x y z) (values 1 2 3)]) (list x y z)) '(1 2 3))
    (void)
    (exit 4))

(if (equal? (let-values ([() (values)]
                         [(x) (values 1)]
                         [(y z) (values 2 3)])
              (+ x y z))
            6)
    (void)
    (exit 5))

(exit 0)
