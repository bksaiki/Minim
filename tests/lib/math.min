;; Tests for <lib>/private/math.min

;; exact-integer
(exact-integer? 1)
(not (exact-integer? 1.0))
(not (exact-integer? 'str))

;; exact-nonnegative-integer?
(exact-nonnegative-integer? 5)
(exact-nonnegative-integer? 0)
(not (exact-nonnegative-integer? 1.0))
(not (exact-nonnegative-integer? -5))

;; exact-positive-integer?
(exact-positive-integer? 5)
(not (exact-positive-integer? 0))
(not (exact-positive-integer? 1.0))
(not (exact-positive-integer? -5))

;; factorial?
(equal? (factorial 0) 1)
(equal? (factorial 1) 1)
(equal? (factorial 2) 2)
(equal? (factorial 4) 24)

;; fibonacci
(equal? (fibonacci 0) 1)
(equal? (fibonacci 1) 1)
(equal? (fibonacci 2) 2)
(equal? (fibonacci 4) 5)

;; sqr
(equal? (sqr 1) 1)
(equal? (sqr 2) 4)
(equal? (sqr 3.0) 9.0)
(equal? (sqr 5/2) 25/4)

;; sgn
(equal? (sgn 5) 1)
(equal? (sgn 0) 0)
(equal? (sgn -3) -1)
(equal? (sgn 5.0) 1.0)
(equal? (sgn 0.0) 0.0)
(equal? (sgn -3.0) -1.0)

;; add1
(equal? (add1 1) 2)
; (equal? (add1 1.0) 2.0)

;; sub1
(equal? (sub1 2) 1)
; (equal? (sub1 2.0) 1.0)
